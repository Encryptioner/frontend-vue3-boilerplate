{
  "name": "frontend-vue3-boilerplate",
  "version": "3.0.0",
  "private": true,
  "engines": {
    "node": "^16.13"
  },
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "build:preview": "vite build && vite preview",
    "start": "pnpm dev & wait-on tcp:3000 -v",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "test": "vitest",
    "test-ct": "playwright test -c playwright-ct.config.ts",
    "test-e2e": "playwright test -c playwright.config.ts",
    "test:ci": "vitest",
    "test:ci-e2e": "playwright test",
    "test:ci-ct": "playwright test -c playwright-ct.config.ts"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.6",
    "@vueuse/core": "^8.3.1",
    "@vueuse/head": "0.7.6",
    "daisyui": "^2.14.3",
    "nprogress": "^0.2.0",
    "pinia": "2.0.14",
    "pinia-plugin-persistedstate": "^1.6.1",
    "sass": "^1.50.1",
    "theme-change": "^2.0.2",
    "vue": "3.2.36",
    "vue-router": "4.0.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@pinia/testing": "0.0.12",
    "@playwright/experimental-ct-vue": "1.22.2",
    "@playwright/test": "1.22.2",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/line-clamp": "0.4.0",
    "@tailwindcss/typography": "0.5.2",
    "@types/node": "17.0.39",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "@vitejs/plugin-vue": "2.3.3",
    "@vue/test-utils": "2.0.0",
    "autoprefixer": "10.4.7",
    "cssnano": "5.1.10",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-security": "1.5.0",
    "eslint-plugin-vue": "9.1.0",
    "happy-dom": "5.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "postcss": "8.4.14",
    "postcss-import": "14.1.0",
    "postcss-nesting": "10.1.7",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "tailwindcss": "3.1.3",
    "typescript": "4.7.2",
    "unplugin-auto-import": "0.8.6",
    "unplugin-vue-components": "0.19.6",
    "vite": "2.9.9",
    "vite-plugin-checker": "^0.4.6",
    "vitest": "0.13.1",
    "vue-tsc": "^0.37.9",
    "wait-on": "6.0.1"
  },
  "lint-staged": {
    "*.{ts,tsx,vue,js,jsx}": "eslint --cache --fix"
  }
}